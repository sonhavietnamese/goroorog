{
  "name": "goroorog",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "program:build": "anchor build",
    "program:deploy": "solana program deploy target/deploy/goroorog.so --use-rpc https://rpc.gorbagana.wtf"
  },
  "devDependencies": {
    "prettier": "^3.6.0",
    "turbo": "^2.5.4",
    "typescript": "5.8.2",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "ts-mocha": "^10.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.12",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@solana/web3.js": "^1.98.2"
  }
}